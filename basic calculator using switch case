#include <iostream>
using namespace std;
int main()
{
    int a;
    int b;
    cout << "enter a:" << endl;
    cin >> a;
    cout << "enter b:" << endl;
    cin >> b;
    char ch;
    cout << "enter the operator: " << endl;
    cin >> ch;
    switch (ch)
    {
    case '+':

        cout << (a + b) << endl;
        break;
    case '-':

        cout << (a - b) << endl;
        break;
    case '*':
        cout << (a * b) << endl;
        break;
    case '/':
        cout << (a / b) << endl;
        break;
    default:
        cout << "invalid operator" << endl;
    }
}
